                  <script type="text/javascript">
                  P.when('A', 'a-modal', 'a-checkbox', 'ready').execute(function(A, modal, getCheckboxCollection) {

                    var $ = A.$;

                    function logRefTag(refTag) {
                      $.get("/gp/product/ajax-handlers/reftag.html/ref=" + refTag);
                    }

                    function hidePopover(index) {
                      var popover = modal.get("mbbPopover" + index);
                      if (popover != null) {
                        popover.hide();
                      }
                    }

                    function logCheckBoxRefTag(isChecked, index, warrantyAsin) {
                      var suffix = (index+1) + "_B01NCE2FHK_" + warrantyAsin;
                      if (isChecked) {
                        logRefTag("psd_bb_y" + suffix);
                      } else {
                        logRefTag("psd_bb_n" + suffix);
                      }
                    }

                    $("#buybox .js-abbNew input[id^=mbb-offeringID-]").click(function() {
                      var flag = this.checked;





                      getCheckboxCollection('input[id^=mbb-offeringID-]').uncheck();


                      this.checked = flag;
                      var index = $('#buybox .js-abbNew input[id^=mbb-offeringID-]').index(this);
                      var warrantyAsin = $(this).parents('.abbListItem').find('input[name^=asin]').val();
                      logCheckBoxRefTag(flag, index, warrantyAsin);
                    });

                    $("#buybox #quantity").change(function() {
                      var quantity = $(this).val();
                      $("#buybox .js-abbNew input[name^=quantity]").each(function() {
                        this.value = quantity;
                      });
                    });

                    logRefTag("psd_bb_i_B01NCE2FHK");


                    logRefTag("psd_bb_i1_B01NCE2FHK_B017KKON8M");
                    $(document).undelegate('#mbbAddCoverage1', 'click');
                    $(document).delegate('#mbbAddCoverage1', 'click', function() {



                      getCheckboxCollection('input[id^=mbb-offeringID-]').uncheck();


                      getCheckboxCollection('input[id^=mbb-offeringID-1]').check();
                      logRefTag('psd_bb_y_B01NCE2FHK_B017KKON8M');
                      hidePopover("1");
                    });
                    $(document).undelegate('#mbbNoCoverage1', 'click');
                    $(document).delegate('#mbbNoCoverage1', 'click', function() {
                      getCheckboxCollection('input[id^=mbb-offeringID-1]').uncheck();
                      logRefTag('psd_bb_n_B01NCE2FHK_B017KKON8M');
                      hidePopover("1");
                    });
                    $(document).undelegate('#mbbPopoverLink1', 'click');
                    $(document).delegate('#mbbPopoverLink1', 'click', function() {
                      logRefTag('psd_bb_pu1_B01NCE2FHK_B017KKON8M');
                    });


                  });
                  </script>










































































